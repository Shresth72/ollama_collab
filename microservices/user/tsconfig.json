{
    "compilerOptions": {
      "strict": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "ES6",
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "baseUrl": ".",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
    }
  }
  